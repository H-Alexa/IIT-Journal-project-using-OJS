msgid "plugins.generic.webfeed.displayName"
msgstr "Модуль веб-стрічок"

msgid "plugins.generic.webfeed.description"
msgstr "Створює стрічку веб-синдикації у форматах RSS/Atom для поточного випуску."

msgid "plugins.generic.webfeed.settings"
msgstr "Налаштування"

