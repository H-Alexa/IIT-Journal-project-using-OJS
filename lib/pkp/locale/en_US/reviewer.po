msgid "reviewer.submission.declineReview"
msgstr "Decline Review Request"

msgid "reviewer.submission.declineReviewMessage"
msgstr "You may provide the editor with any reasons why you are declining this review in the field below."

msgid "reviewer.submission.acceptReview"
msgstr "Accept Review, Continue to Step #2"

msgid "reviewer.submission.continueToStepThree"
msgstr "Continue to Step #3"

msgid "reviewer.submission.submitReview"
msgstr "Submit Review"

